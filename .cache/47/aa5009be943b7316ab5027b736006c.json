rgin-bottom: 2rem;\r\n        text-align: center;\r\n        font-size: 14px;\r\n        line-height: 1.57;\r\n    }\r\n\r\n    .email_catcher{\r\n        position: relative;\r\n\r\n        input{\r\n            display: block;\r\n            width: 100%;\r\n            padding: .7rem 1.5rem;\r\n            color: rgba(66, 58, 58, 1);\r\n\r\n            &::placeholder{\r\n                font-size: 14px;\r\n                color: $textColor;\r\n            }\r\n        }\r\n\r\n        // input:invalid{\r\n        //     border: solid 2px $errorColor;\r\n        // }\r\n\r\n        .btn_validate{\r\n            position: absolute;\r\n            right: 0;\r\n            top: 50%;\r\n            transform: translateY(-50%);\r\n            padding: 7px 1.5rem;\r\n            background: $linearColor;\r\n            border: solid 1px $textColor;\r\n            border-radius: 28px;\r\n            box-shadow: 0 15px 20px 0 rgba(198, 110, 110, 0.24);\r\n        }\r\n\r\n        .error_svg{\r\n            position: absolute;\r\n            right: 23%;\r\n            top: 50%;\r\n            transform: translate(-50%,-50%);\r\n            opacity: 0;\r\n        }\r\n    }\r\n}\r\n\r\n.error_text{\r\n    order: 4;\r\n    margin-left: 3.5rem;\r\n    margin-bottom: 3rem;\r\n    opacity: 0;\r\n    font-size: 13px;\r\n    color: $errorColor;\r\n}\r\n\r\n//* ///////////////////IMG CONTAINER /////////////////////\r\n\r\n.img_container{\r\n    order: 2;\r\n    margin-bottom: 2rem;\r\n}\r\n\r\n//! ////////////////// MEDIA QUERIES (DESKTOP) //////////////////////\r\n\r\n@media only screen and (min-width: 600px){\r\n    body{\r\n        height: 100vh;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n    \r\n    .website{\r\n        max-width: 1440px;\r\n        max-height: 800px;\r\n        display: grid;\r\n        grid-template-columns: 2fr auto;\r\n        grid-template-rows: 20% auto;\r\n        box-shadow: 0 0 20px 0 rgb(156, 156, 156);\r\n    }\r\n\r\n    .brand_container{\r\n        margin: 4.5rem auto 8.5rem 10rem;\r\n    }\r\n\r\n    .content_container{\r\n        margin: 0 13.75rem 0 10rem;\r\n        padding: 0;\r\n\r\n        h1{\r\n            font-size: 4rem;\r\n            text-align: left;\r\n        }\r\n\r\n        p{\r\n            font-size: 1rem;\r\n            text-align: left;\r\n        }\r\n\r\n        .email_catcher{\r\n    \r\n            input{\r\n                font-size: 1rem;\r\n                padding: 1rem 2rem;\r\n            }\r\n\r\n            .btn_validate{\r\n                padding: 12px 2rem;\r\n            }\r\n        }\r\n    }\r\n\r\n    .img_container img{\r\n        max-width: 610px;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"7b598376d81cb8a6419e2a016993a2cd","cacheData":{"env":{}}}